# DailyDevQ 프론트엔드 프로덕션 환경 변수

# 환경 설정
NODE_ENV=production

# 프론트엔드 URL (실제 도메인)
NEXT_PUBLIC_APP_URL=https://dailydevq.com

# 백엔드 API URL (실제 API 도메인)
NEXT_PUBLIC_API_URL=https://api.dailydevq.com

# NextAuth 설정
NEXTAUTH_URL=https://dailydevq.com
NEXTAUTH_SECRET=CHANGE_THIS_TO_STRONG_RANDOM_SECRET_MINIMUM_32_CHARS

# Google OAuth (프로덕션)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Vercel Analytics
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=your-vercel-analytics-id

# Google Analytics
NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX

# 사이트 정보
NEXT_PUBLIC_SITE_NAME=DailyDevQ
NEXT_PUBLIC_SITE_DESCRIPTION=매일 오전 7시, 실무 핵심만 전합니다. Tech Letter.
